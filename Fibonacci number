class Solution {
public:
    int tribonacci(int n) {
         vector<int> t;
        
        t.push_back(0);
        t.push_back(1);
        t.push_back(1);
        for(int i=3;i<=n;i++)
        {
            t.push_back(t.at(i-1)+t.at(i-2)+t.at(i-3));
        }        
        return t[n];      
    
        
        
    }
};

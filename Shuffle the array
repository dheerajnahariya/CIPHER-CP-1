class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        
        vector<int> result;
        int m=nums.size();
        for (int i=0,j=n;j<2*n;i++,j++)
        {
            result.push_back(nums[i]);
            result.push_back(nums[j]);
        }
        
        return result; 
    }
};
